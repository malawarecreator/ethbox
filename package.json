{
  "name": "ethbox-cli",
  "description": "Ethereum CLI toolkit",
  "version": "0.0.5",
  "author": "@malawarecreator",
  "bin": {
    "ethbox": "bin/run.js"
  },
  "bugs": "https://github.com/malawarecreator/ethbox/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "viem": "^2.24.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.12",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^17.0.4",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^20.0.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/malawarecreator/ethbox",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "ethbox",
    "dirname": "ethbox",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "wallet": {
        "description": "wallet actions"
      },
      "tx": {
        "description": "transaction actions"
      },
      "auth": {
        "description": "authentication actions"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malawarecreator/ethbox.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && oclif manifest",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
